<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/cs2"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/topTeamList"
        android:layout_width="@dimen/size_x"
        android:layout_height="@dimen/size_y"
        android:divider="@color/gray"
        android:dividerHeight="@dimen/dividerHeight"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="@dimen/Horizontal_bias"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="@dimen/Vertical_bias">
    </ListView>
</androidx.constraintlayout.widget.ConstraintLayout>